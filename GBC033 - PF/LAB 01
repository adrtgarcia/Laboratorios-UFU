dobro :: Float -> Float
dobro x = 2 * x

quadruplo :: Float -> Float
quadruplo x = (dobro x) * 2

soma2 :: Float -> Float -> Float
soma2 x y = x + y

soma4 :: Float -> Float -> Float -> Float -> Float
soma4 a b c d = (soma2 a b) + (soma2 c d)

misterio :: Float -> Float -> Float -> Float -> Float
misterio x y z w = soma2 (soma2 x y) (soma2 z w)

hipotenusa :: Float -> Float -> Float
hipotenusa ca cb = sqrt (soma2 (ca**2) (cb**2))
